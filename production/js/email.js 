$(document).ready(function(){$("#send_message").click(function(a){a.preventDefault();var b=!1,c=$("#exampleInputEmail").val(),d=$("#exampleInputSubject").val(),e=$("#exampleInputMessage").val();if(0==c.length||"-1"==c.indexOf("@")){var b=!0;$("#exampleInputEmail").addClass("validation")}else $("#exampleInputEmail").removeClass("validation");if(0==d.length){var b=!0;$("#exampleInputSubject").addClass("validation")}else $("#exampleInputSubject").removeClass("validation");if(0==e.length){var b=!0;$("#exampleInputMessage").addClass("validation")}else $("#exampleInputMessage").removeClass("validation");0==b&&($("#send_message").attr({enabled:"enable",value:"Sending..."}),$.post("php/email.php",$("#contact_form").serialize(),function(a){"sent"==a?($("#send_message").attr({enabled:"enable",value:"send"}),$("#mail_success").fadeIn(500)):($("#mail_fail").fadeIn(500),$("#send_message").removeAttr("disabled").attr("value","Send The Messagetwo"))}))})});